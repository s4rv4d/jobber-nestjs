generator client {
    provider = "prisma-client-js"
    binaryTargets = ["native", "linux-arm64-openssl-3.0.x", "debian-openssl-3.0.x"]
    output = "../../../node_modules/@prisma-clients/auth"
}

datasource db {
    provider = "postgresql"
}

model User {
    id Int @id @default(autoincrement())
    email String @unique
    password String
}